[user]
  name = mibu5
  email = mikolaj.burawski@gmail.com
[core]
  editor = vi
  excludesfile = ~/.gitignore
  autocrlf = input
[pretty]
	my = "format:%C(yellow)%h %C(dim green)%ad %C(reset)| %C(cyan)%s%d %C(#667788)[%an]"
[format]
    pretty = my
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[init]
  defaultBranch = main
[difftool]
  prompt = false
[alias]
  aa = add --all
  c = commit
  ca = commit -a
  cm = commit -m
  cam = commit -am
  d = diff
  dc = diff --cached
  l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit

